; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Where is the Princess?"
run/main_scene="res://splash_screen/splash_screen_manager.tscn"
config/custom_user_dir_name="Where is the Princess"
config/features=PackedStringArray("4.2")
boot_splash/image="res://wekufu logo screen.png"
config/icon="res://icon.png"

[autoload]

ModLoaderStore="*res://addons/mod_loader/mod_loader_store.gd"
ModLoader="*res://addons/mod_loader/mod_loader.gd"
SceneTransistor="*res://Autoloads/SceneTransistor.tscn"
SavedData="*res://Autoloads/saved_data/SavedData.gd"
Settings="*res://Autoloads/Settings/Settings.tscn"
Globals="*res://Autoloads/Globals.gd"
InfoPanel="*res://Autoloads/info_panel/info_panel.tscn"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
Log="*res://addons/logger/logger.gd"

[debug]

gdscript/warnings/property_used_as_function=2
gdscript/warnings/constant_used_as_function=2
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1

[display]

window/size/viewport_width=432
window/size/viewport_height=243
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"
mouse_cursor/custom_image="res://Art/16x16 Pixel Art Roguelike (Forest) Pack/ui/cursor.png"

[dotnet]

project/assembly_name="Godot4RoguelikeProject"

[editor]

run/main_run_args="-- --debug --skip-splash-and-menu --log-level debug"

[editor_plugins]

enabled=PackedStringArray("res://addons/beehave/plugin.cfg", "res://addons/csv-data-importer/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/logger/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "green",
"res://Audio/": "pink",
"res://Autoloads/": "yellow",
"res://Characters/": "gray",
"res://Rooms/": "red",
"res://Translations/": "purple",
"res://Weapons/": "teal",
"res://addons/": "blue",
"res://scripts/": "orange",
"res://test/": "blue"
}

[filesystem]

import/blender/enabled=false

[gdunit4]

templates/testsuite/GDScript="# GdUnit generated TestSuite
class_name ${suite_class_name}
extends GdUnitTestSuite
@warning_ignore('unused_parameter')
@warning_ignore('return_value_discarded')

# TestSuite generated from
const __source: String = '${source_resource_path}'
"

[global]

version="0.1.0"

[gui]

fonts/dynamic_fonts/use_oversampling=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

